<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block name="government-list" template="Chilliapple_Governments::government/list.phtml">
                <arguments>
                    <argument name="listGovernment" xsi:type="object">Chilliapple\Governments\ViewModel\Government\ListGovernment</argument>
                    <argument name="formatter" xsi:type="object">Chilliapple\Governments\ViewModel\Formatter</argument>
                    <argument name="governmentUrl" xsi:type="object">Chilliapple\Governments\ViewModel\Government\Url</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
