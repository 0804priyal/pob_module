<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Chilliapple\Redirect\Controller\Adminhtml\Redirect\Index">
        <arguments>
            <argument name="activeMenuItem" xsi:type="string">Chilliapple_Redirect::redirect_redirect</argument>
            <argument name="pageTitle" xsi:type="string" translate="true">Redirects</argument>
        </arguments>
    </type>
    <type name="Chilliapple\Redirect\Controller\Adminhtml\Redirect\MassDelete">
        <arguments>
            <argument name="collectionProvider" xsi:type="object">Chilliapple\Redirect\Model\Redirect\CollectionProvider</argument>
            <argument name="executor" xsi:type="object">Chilliapple\Redirect\Model\Redirect\Executor\Delete</argument>
            <argument name="successMessage" xsi:type="string">A total of %1 Redirects have been deleted.</argument>
            <argument name="errorMessage" xsi:type="string" translate="true">An error occurred while deleting Redirects.</argument>
        </arguments>
    </type>
    <type name="Chilliapple\Redirect\Controller\Adminhtml\Redirect\Delete">
        <arguments>
            <argument name="executor" xsi:type="object">Chilliapple\Redirect\Model\Redirect\Executor\Delete</argument>
            <argument name="paramName" xsi:type="string">redirect_id</argument>
            <argument name="successMessage" xsi:type="string" translate="true">Redirect was deleted</argument>
            <argument name="missingEntityErrorMessage" xsi:type="string" translate="true">Requested Redirect for delete does not exist.</argument>
            <argument name="generalErrorMessage" xsi:type="string" translate="true">There was a problem deleting the Redirect</argument>
        </arguments>
    </type>
</config>
